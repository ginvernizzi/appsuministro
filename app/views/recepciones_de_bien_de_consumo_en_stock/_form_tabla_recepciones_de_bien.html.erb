<% if !@recepciones_de_bien_de_consumo.nil? && @recepciones_de_bien_de_consumo.length > 0 %>
<table class="table">
  <thead>
    <tr>
      <th>Numero</th>
      <th>Fecha</th>      
      <th>Tipo de Documento</th>
      <th>Documento principal</th>
      <th colspan="2"> </th>
    </tr>
  </thead>

  <tbody>
    <% @recepciones_de_bien_de_consumo.each do |recepcion_de_bien_de_consumo| %>      
    <tr>
      <td><%= recepcion_de_bien_de_consumo.id %></td>
      <td><%= recepcion_de_bien_de_consumo.fecha %></td>        
      <% if (recepcion_de_bien_de_consumo.documento_principal) %>
        <td><%= recepcion_de_bien_de_consumo.documento_principal.documento_de_recepcion.tipo_de_documento.nombre %></td>
        <td><%= recepcion_de_bien_de_consumo.documento_principal.documento_de_recepcion.numero_de_documento %></td>
      <% else %>
        <td></td>
        <td></td>
      <% end %>      
        <td><%= link_to 'Ver',  recepciones_de_bien_de_consumo_en_stock_path(recepcion_de_bien_de_consumo)%></td>       
    </tr>
    <% end %>

    <% if(@recepciones_de_bien_de_consumo.length > 0) %>              
            <td><%= link_to 'Imprimir', imprimir_formulario_recepciones_por_documento_principal_fecha_path(@recepciones_de_bien_de_consumo.first().documento_principal.documento_de_recepcion.numero_de_documento, @recepciones_de_bien_de_consumo.first().fecha_inicio, @recepciones_de_bien_de_consumo.first().fecha_fin) , method: :post, data: { confirm: 'Â¿Esta seguro?' }  %>        
            </td>   
        <% end %>      
  </tbody>
</table>
<% else %>
  <h3> No hay resultados para mostrar </h3>  
<% end %>
<br>
