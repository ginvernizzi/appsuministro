<%= form_for(@partida_principal) do |f| %>
  <% if @partida_principal.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@partida_principal.errors.count, "error/es") %> La Partida Principal no puedo ser guardada:</h2>

      <ul>
      <% @partida_principal.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  
  <div class="field">
      <%= f.label :inciso%><br /> 
      <%= f.collection_select :inciso_id, @incisos, :id, :nombre, :prompt => "seleccione..." %>  
  </div>
  <div class="field">
    <%= f.label :codigo, "Codigo (1 caracter)" %><br>
    <%= f.text_field :codigo %>
  </div>
  <div class="field">
    <%= f.label :nombre %><br>
    <%= f.text_area :nombre %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
