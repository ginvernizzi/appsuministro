<h1>Consumos directos</h1>

<table class="table">
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Area</th>
      <th>Obra proyecto</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @consumos_directo.each do |consumo_directo| %>
      <tr>
        <td><%= consumo_directo.fecha %></td>
        <td><%= consumo_directo.area.nombre %></td>
        <td><%= consumo_directo.obra_proyecto.descripcion %></td>
        <td><%= link_to 'Ver', consumo_directo %></td>
        <td><%= link_to 'Imprimir',  imprimir_consumos_directo_recepciones_de_bien_de_consumo_consumidas_path(consumo_directo), method: :post, data: { confirm: 'Â¿Esta seguro?' }  %>        
       </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
