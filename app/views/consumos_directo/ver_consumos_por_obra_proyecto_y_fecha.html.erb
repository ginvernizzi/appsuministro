<h1> Listado de Consumos por Obra proyecto </h1>

<div class="row">
  <form class="navbar-form navbar-left" role="search">    
    <div class="form-group">    
      <div class"field">
        <%= label_tag 'seleccione un rango de fechas'%>  
      </div>
      <br />
      <div class"field">
        <%= label :fecha_inicio, "Desde" %>
      </div>

      <div class"field">
        <%= text_field_tag :fecha_inicio, nil, readonly: true %>  
      </div>

      <div class"field">
       <%= label :fecha_fin, "Hasta" %>
      </div>

      <div class"field">
       <%= text_field_tag :fecha_fin, nil, readonly: true %>        
      </div>
    </div>
  </form>  
</div> 

<div class="field">
   <%= label :obra_proyecto, "Obra proyecto" %> <br />   
</div>   
<div class="field">
    <%= autocomplete_field_tag 'obra_proyecto_descripcion', '', autocomplete_obra_proyecto_descripcion_consumos_directo_path , :size => 75 %>
</div> 

       
<div>
  <%= hidden_field_tag 'obra_proyecto_id' %>  
</div>

<div class="field">
  <input type="button" value="Obtener" class="" id="obtener_lista_de_consumos_por_obra_proyecto_y_fecha">        
</div>

<div id="tabla_bienes">    
  <%= render 'form_tabla_bienes' %>   
</div> 


