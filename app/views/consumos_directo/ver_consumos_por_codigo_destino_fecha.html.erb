<h1>Listado de Consumos por Bien y Destino</h1>

<div class="row">
  <form class="navbar-form navbar-left" role="search">    
    <div class="form-group">    
      <div class"field">
        <%= label_tag 'seleccione un rango de fechas'%>  
      </div>
      <br />
      <%= label :fecha_inicio, "Desde" %>
      <%= text_field_tag :fecha_inicio, nil, readonly: true %>  
      <%= label :fecha_fin, "Hasta" %>
      <%= text_field_tag :fecha_fin, nil, readonly: true %>        
    </div>
  </form>  
</div> 

<div class="field">
   <%= label :Bien_de_consumo, "Bien de consumo" %> <br />   
</div>   
<div class="field">
    <%= autocomplete_field_tag 'bien_de_consumo_nombre', '', autocomplete_bien_de_consumo_nombre_items_stock_path , :size => 75 %>
</div> 

<div class="field">
   <%= label :Area, "Area destino" %> <br />   
</div>  
<div class="field">
    <%= autocomplete_field_tag 'area_nombre', '', autocomplete_area_nombre_items_stock_path , :size => 75 %>
</div> 
       
<div>
  <%= hidden_field_tag 'bien_de_consumo_id' %>
  <%= hidden_field_tag 'area_id' %>
</div>

<div class="field">
  <input type="button" value="Obtener" class="" id="obtener_lista_de_consumos_por_codigo_destino_y_fecha">        
</div>

<div id="tabla_bienes">    
  <%= render 'form_tabla_bienes' %>   
</div> 


