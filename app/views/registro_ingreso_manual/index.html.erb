<h1>Reporte de ingresos a stock manualmente</h1>

<div class="row">
  <form class="navbar-form navbar-left" role="search">
    <div class="form-group">
      <div class"field">
        <%= label_tag 'seleccione un rango de fechas'%>
      </div>
      <br />
      <div class"field">
        <%= label :fecha_inicio, "Desde" %>
      </div>
      <div class"field">
        <%= text_field_tag :fecha_inicio, nil, readonly: true %>
      </div>
      <div class"field">
       <%= label :fecha_fin, "Hasta" %>
      </div>
      <div class"field">
       <%= text_field_tag :fecha_fin, nil, readonly: true %>
      </div>
    </div>
  </form>
</div>

<div class="field">
  <input type="button" value="Obtener" class="" id="traer_lista_de_ingresos_manuales">
</div>
<br>
<div id="tabla_ingresos_manuales">
  <%= render 'form' %>
</div>
