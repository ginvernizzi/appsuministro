<h1>Lista de obras/proyectos</h1>

<table class="table">
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @obras_proyectos.each do |obra_proyecto| %>
      <tr>
        <td><%= obra_proyecto.id %></td>
        <td><%= obra_proyecto.descripcion %></td>
        <td><%= link_to 'Ver', obra_proyecto %></td>
        <td><%= link_to 'Modificar', edit_obra_proyecto_path(obra_proyecto) %></td>
        <td><%= link_to 'Eliminar', obra_proyecto, method: :delete, data: { confirm: 'Â¿Esta seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nueva Obra/Proyecto', new_obra_proyecto_path %>
