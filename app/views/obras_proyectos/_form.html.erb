<%= form_for(@obra_proyecto) do |f| %>
  <% if @obra_proyecto.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@obra_proyecto.errors.count, "error/es") %> El item no pudo ser guardado:</h2>
      <ul>
      <% @obra_proyecto.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :codigo %><br>
    <%= f.text_field :codigo %>
  </div>
  <div class="field">
    <%= f.label :descripcion %><br>
    <%= f.text_field :descripcion %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
