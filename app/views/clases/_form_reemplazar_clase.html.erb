<%= form_for(@clase, 
             url:  dar_de_baja_y_reemplazar_clases_path, 
             html: {method: "put"}) do |f| %>
  <% if @clase.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@clase.errors.count, "error") %> clase no pudo ser reemplzada correctamente:</h2>

      <ul>
      <% @clase.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  
 <div class="row">
  <div class="col-md-4">  
      <div class="field">
          <%= f.label :partida_parcial%><br /> 
          <%= f.collection_select :partida_parcial_id, @partidas_parciales, :id, :nombre, :prompt => "seleccione..." %>  
      </div>
      <div class="field">
        <%= f.label :codigo, "Codigo (5 caracteres)" %><br>
        <%= f.text_field :codigo %>
      </div>
      <div class="field">
        <%= f.label :nombre %><br>
        <%= f.text_area :nombre %>
        <input type="button" value="Buscar similares" id="buscar_nombres_repetidos">   
      </div>

      <div class="actions">
        <%= f.submit %>
      </div>
      <%= link_to 'Volver', clases_path %>
    </div>
    <div class="col-md-4">  
      <div id="titulo"> </div> 
      <div id="tabla_items_existentes"> </div>   
    </div>   
  </div>
<% end %>
